{
	"name": "node-backend-template",
	"version": "1.0.0",
	"description": "template for node-typescript-expressjs with pm2",
	"main": "index.js",
	"author": "stilyng94",
	"license": "MIT",
	"scripts": {
		"build": "rm -rf ./dist && tsc",
		"lint": "prettier --write --ignore-unknown . && eslint --fix --ext src/*.{js,ts,json}",
		"start": "node ./dist/index.js",
		"dev": "nodemon -L ./src/index.ts",
		"prepare": "husky install",
		"test": "jest --detectOpenHandles --no-cache -i",
		"prototype": "prisma db push --accept-data-loss",
		"studio": "prisma studio",
		"generate": "prisma generate",
		"format": "prisma format && pnpm generate",
		"reset:db": "prisma migrate reset --skip-seed",
		"migrate:deploy": "prisma migrate deploy",
		"migrate:dev": "prisma migrate dev --skip-seed",
		"seed": "pnpm prisma db seed",
		"seed:dev": "ts-node ./src/seed/seed.ts",
		"worker:dev": "ts-node ./src/worker.ts",
		"worker": "node ./dist/worker.js"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@tsconfig/recommended": "^1.0.1",
		"@types/connect-redis": "^0.0.18",
		"@types/cors": "^2.8.12",
		"@types/ejs": "^3.1.1",
		"@types/express": "^4.17.14",
		"@types/express-session": "^1.17.5",
		"@types/jest": "^29.1.2",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.8.0",
		"@types/nodemailer": "^6.4.6",
		"@types/passport": "^1.0.11",
		"@types/passport-local": "^1.0.34",
		"@types/passport-oauth2": "^1.4.11",
		"@types/supertest": "^2.0.12",
		"@types/ua-parser-js": "^0.7.36",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.39.0",
		"eslint": "^8.0.1",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard-with-typescript": "^23.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-jest": "^27.1.1",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.0.0",
		"husky": "^8.0.1",
		"jest": "^29.1.2",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.20",
		"pino-pretty": "^9.1.0",
		"prettier": "^2.7.1",
		"supertest": "^6.3.0",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "*"
	},
	"dependencies": {
		"@godaddy/terminus": "^4.11.2",
		"@prisma/client": "^4.7.1",
		"@sentry/node": "^7.25.0",
		"axios": "^1.2.1",
		"bullmq": "^3.4.1",
		"connect-redis": "^6.1.3",
		"cors": "^2.8.5",
		"cron-validate": "^1.4.5",
		"ejs": "^3.1.8",
		"express": "^4.18.1",
		"express-session": "^1.17.3",
		"express-validator": "^6.14.2",
		"google-auth-library": "^8.6.0",
		"helmet": "^6.0.1",
		"hpp": "^0.2.3",
		"ioredis": "^5.2.3",
		"jsonwebtoken": "^8.5.1",
		"jwks-rsa": "^3.0.0",
		"multer": "^1.4.5-lts.1",
		"nodemailer": "^6.8.0",
		"passport": "^0.6.0",
		"passport-local": "^1.0.0",
		"passport-oauth2": "^1.6.1",
		"pino": "^8.6.1",
		"pino-http": "^8.2.1",
		"pino-sentry": "^0.13.0",
		"prisma": "^4.7.1",
		"rate-limiter-flexible": "^2.3.12",
		"znv": "^0.3.2",
		"zod": "^3.20.2"
	},
	"engines": {
		"node": ">=16.16.0"
	},
	"engineStrict": true,
	"lint-staged": {
		"src/*.{js,ts}": [
			"pnpm lint"
		]
	},
	"prisma": {
		"seed": "node ./dist/seed/seed.js"
	}
}
